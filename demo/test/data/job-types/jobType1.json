{
  "id": 1,
  "name": "landsat-parse",
  "version": "1.0.0",
  "title": "Landsat 8 Parse",
  "description": "Parse Landsat 8 multi-tif files in tar.gz archives",
  "category": "Landsat",
  "is_operational": true,
  "icon_code": "f248",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/landsat-parse_1.0:dev",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 0.25,
  "mem_required": 512.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "multispectral"
    }, {
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "panchromatic"
    }, {
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "thermal"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["application/octet-stream"],
      "required": true,
      "type": "file",
      "name": "infile"
    }],
    "version": "1.0",
    "command": "./parse_landsat.sh"
  },
  "error_mapping": {}
}