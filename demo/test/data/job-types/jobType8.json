{
  "id": 8,
  "name": "ls8-ortho",
  "title": "Landsat 8 Ortho",
  "version": "2.1.9",
  "description": "Applies Orthorectification to Landsat 8 data",
  "category": "spectral",
  "is_operational": true,
  "icon_code": "f248",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/ls8-ortho:2.1.9",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 4.0,
  "mem_required": 2048.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "ls8-corrected"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["application/octet-stream"],
      "required": true,
      "type": "file",
      "name": "infile"
    }],
    "version": "1.0.1",
    "command": "./ls8_ortho.py"
  },
  "error_mapping": {}
}