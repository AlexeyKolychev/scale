{
  "id": 5,
  "name": "wv2-parse",
  "title": "WorldView-2 Parse",
  "version": "1.0.1",
  "description": "Parses WorldView-2 GeoTiffs",
  "category": "spectral",
  "is_operational": true,
  "icon_code": "f248",
  "docker_privileged": false,
  "docker_image": "10.4.4.10:5000/wv2-parse:1.0.1",
  "priority": 1,
  "timeout": 30,
  "max_tries": 3,
  "cpus_required": 0.25,
  "mem_required": 512.0,
  "disk_out_const_required": 2048.0,
  "disk_out_mult_required": 0.0,
  "interface": {
    "output_data": [{
      "media_type": "image/tiff",
      "required": true,
      "type": "file",
      "name": "wv2-parsed"
    }],
    "shared_resources": [],
    "command_arguments": "${infile} ${job_output_dir}",
    "input_data": [{
      "media_types": ["application/octet-stream"],
      "required": true,
      "type": "file",
      "name": "infile"
    }],
    "version": "1.0.1",
    "command": "./parse_wv2.sh"
  },
  "error_mapping": {}
}