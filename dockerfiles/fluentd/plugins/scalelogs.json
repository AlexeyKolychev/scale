{
  "template": "scalelogs-*",
  "mappings": {
    "scalelogs": {
      "_all": {
        "enabled": false
      },
      "_source": {
        "compress": true
      },
      "properties": {
        "@timestamp": {
          "format": "strict_date_optional_time||epoch_millis",
          "type": "date"
        },
        "container_name": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        },
        "host": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "scale_job_exe": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        },
        "scale_job_exe_num": {
          "type": "long"
        },
        "scale_job_id": {
          "type": "long"
        },
        "scale_job_type": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        },
        "scale_job_version": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        },
        "scale_order_num": {
          "type": "long"
        },
        "stream": {
          "fields": {
            "raw": {
              "index": "not_analyzed",
              "type": "string"
            }
          },
          "index": "analyzed",
          "type": "string"
        }
      }
    }
  }
}